<!--dropdowna göre getirme scripti-->
<script type="text/javascript">
    function GetUserDetails(obj) {
        var selectedUser = $(obj).val();
        var urlToHit = '/Toleyis/IlSec';
        $.ajax({
            url: urlToHit,
            cache: false,
            type: "POST",
            data: { id: selectedUser },
            success: function (data) {
                $("#dvUserDetails").html(data);
            },
            error: function () {
                alert("failed");
            }
        });
    }
</script>