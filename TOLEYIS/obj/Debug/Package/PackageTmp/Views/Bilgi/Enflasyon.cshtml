
@{
    ViewBag.Title = "Enflasyon";
    Layout = "~/Views/Shared/DuzYaziLayout.cshtml";
    ViewBag.Baslik = "Enflasyon Bilgileri";
    ViewBag.Sekme = "Bilgi";
}

<link href="~/assets/css/style.css" rel="stylesheet" />
<!--Page Header End-->
<!--News Details Start-->
<section class="news-details">
    <div class="container">
        <div class="row">
            @Html.Action("Sidebar", "Bilgi")

            <div class="col-md-9" style="font-size: 17px;">
                <h5 style=" text-align: -webkit-center; ">Geçmiş Veriler</h5><br />
                <div class="table-responsive-md">
                    <table class="table">
                        <thead>
                            <tr>
                                <th scope="col">Ay</th>
                                <th scope="col">1 Aylık %</th>
                                <th scope="col">6 Aylık %</th>
                                <th scope="col">12 Aylık %</th>
                            </tr>
                        </thead>
                        @foreach (var m in Model)
                        {                            
                            <tbody>
                                <tr>
                                    @if (m.Tarih != null)
                                    {
                                        <td data-order="1561723222">@m.Tarih.ToString("MMMM yyyy")</td>
                                        <td>@m.Aylik1</td>
                                        <td>@m.Aylik6</td>
                                        <td>@m.Aylik12</td>
                                    }

                                </tr>
                            </tbody>
                        }
                    </table>
                </div>

            </div>
        </div>
    </div>
</section>
<!--News Details End-->
