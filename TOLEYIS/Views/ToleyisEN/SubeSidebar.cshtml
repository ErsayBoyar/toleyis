
@{
    Layout = null;
}

<style>
    @@media only screen and (max-width: 1025px) {
        .sidebar {
            display: none;
        }
    }
</style>
<!DOCTYPE html>

<div class="col-md-3" style=" padding-right: 35px; ">
    <div class="sidebar">
        <div class="sidebar__single sidebar__category">
            <h3 class="sidebar__title">Suboffices</h3>
            <ul class="sidebar__category-list list-unstyled">
                @foreach (var m in Model)
                {
                    <li><a href="/ToleyisEN/SubeDetay/@m.Id">@m.Ad <span class="fa fa-angle-right"></span></a></li>
                }
            </ul>
        </div>
    </div>
</div>