<!--Haberler Start-->
<section class="popular-causes">
    <div class="container">
        <div class="block-title text-left">
            <h4>Toleyis'ten</h4>
            <h2>Haberler</h2>
        </div>
        <div class="row">
            <div class="col-xl-12">
                <div class="popular-causes__carousel owl-theme owl-carousel">
                    <!--Popular Causes Single-->
                    @foreach (var m in Model.Habers)
                    {
                        <div class="popular-causes__sinlge">
                            <div class="popular-causes__img" style="height:230px;">
                                @if (m.Kapak == null)
                                {
                                    <img src="~/assets/images/backgrounds/calisanlogo.PNG" />
                                }
                                else
                                {
                                    <img src="@ViewBag.dosyayolu/@m.Kapak" alt="" style="height:230px; width:370px;">
                                }
                                <div class="popular-causes__category">
                                    @{ if (m.TurID == 1)
                                        {
                                            <p>@m.HaberKategori.Ad</p>

                                        }
                                        else if (m.TurID == 2)
                                        {
                                            <p>Duyuru</p>
                                        }
                                    }
                                </div>
                            </div>
                            <div class="popular-causes__content" style="height: 120px;">
                                <div class="popular-causes__title">
                                    <p class="list-unstyled news-details__meta" style=" margin-bottom: 10px; ">
                                        <a> @Convert.ToDateTime(m.Tarih).ToString("dd MMMM yyyy")</a>
                                    </p>
                                    <h3>
                                        <a href="/Haberler/HaberDetay/@m.Id" style="overflow: hidden; display: -webkit-box; -webkit-line-clamp: 2; -webkit-box-orient: vertical">@m.BaslikTR</a>
                                    </h3>
                                    @*<p>@Html.Raw(m.texteditorTR)</p>*@
                                </div>
                            </div>
                        </div>
                    }
                </div>
            </div>
        </div>
    </div>
</section>
<!--Haberler End-->
<!--Duyurular Start-->
<section class="counter-one jarallax" data-jarallax data-speed="0.2" data-imgPosition="50% 0%" style="background-image: url(../assets/images/backgrounds/duyuru2.png); padding: 100px 0 45px; ">
